{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.txt",
          "outputTemplate": "{Timestamp:dd/MM/yyyy HH:mm:ss} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    //"DefaultConnection": "Data Source=DESKTOP-JK5I9SJ;Initial Catalog=Betway; Integrated Security=SSPI;TrustServerCertificate=true;"
    "DefaultConnection": "Betway"
  }
}
